int _printf(const char *format, ...)
{
  int i = 0;
  int j;
  int count = 0;
  while (format)
    {
      if (format[i] == '%')
	{
	  i++;
	  j = 0;
	  while (j < 3)
	    {
	      if (format[i] == vars[j])
		{
		  count += f(format[i]);
		  break;
		}
	      j++;
	    }
	  if (((j == 3) && (vars[3])) != format[i])
	    {
	      i--;
	    }
	 
	}
      else
	{
      _putchar(format[i]);
      count++;
	}
      i++;
      



    }

}
